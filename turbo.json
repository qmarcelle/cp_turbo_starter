
{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [".env"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["build"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx"]
    },
    "analyze": {},
    "storybook": {
      "persistent": true
    },
    "test-storybook": {},
    "build-storybook": {
      "outputs": ["storybook-static/**"]
    },
    "e2e:headless": {},
    "e2e:ui": {},
    "coupling-graph": {}
  }
}
